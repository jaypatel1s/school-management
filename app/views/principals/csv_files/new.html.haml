.main-content-container.overflow-hidden
  .d-flex.justify-content-between.align-items-center.flex-wrap.gap-2.mb-4
    %h3.mb-0 User / New
    %div
      %a.ps-0.border-0.bg-transparent.lh-1.position-relative.top-2{ href: college_principals_csv_files_path(current_college.slug), 'data-bs-html': 'true', 'data-bs-offset': '0,4', 'data-bs-original-title':
      '<span>Back</span>', 'data-bs-placement': 'bottom', 'data-bs-toggle': 'tooltip', type: 'button' }
        %i.material-symbols-outlined.fs-16.text-body arrow_back
.row
  .col-xxl-12.col-lg-12
    .card.bg-white.border-0.rounded-3.mb-4
      .card-body.p-4
        %h3.mb-3.mb-lg-4 Add A Csv User
        = render 'form', csv_file: @csv_file, url: college_principals_csv_files_path(current_college.slug)
